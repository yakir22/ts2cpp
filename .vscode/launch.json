{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/js2cpp.ts",
            "stopOnEntry": false,
            "args": ["TestScripts/AStar.ts"],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,            
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal",
			"sourceMaps": true,
			"protocol": "inspector",
            "outFiles": ["${workspaceRoot}/out/*.js","${workspaceRoot}/out/**/*.js"]
        }
    ]
}